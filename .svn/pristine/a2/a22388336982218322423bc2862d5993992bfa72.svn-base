   <select id="selectCustGroupList" resultMap="BaseResultMap" parameterType="com.asiainfo.tfsPlatform.po.TfFCustGroupPo" >
        select 
        <include refid="Base_Column_List" />
        from TF_F_CUST_GROUP
        where 1=1
        <if test="custId != null and custId != ''">
            and CUST_ID = #{custId,jdbcType=VARCHAR}
        </if>
        <if test="custName != null and custName != ''">
            and CUST_NAME = #{custName,jdbcType=VARCHAR}
        </if>
        <if test="groupId != null and groupId != ''">
            and GROUP_ID = #{groupId,jdbcType=VARCHAR}
        </if>
        <if test="juristicName != null and juristicName != ''">
            and JURISTIC_NAME = #{juristicName,jdbcType=VARCHAR}
        </if>
        <if test="removeStaffId != null and removeStaffId != ''">
            and REMOVE_STAFF_ID = #{removeStaffId,jdbcType=VARCHAR}
        </if>
     
    </select>
